{
    "name": "pugx/bindto",
    "description": "Simple way to bind Request to DTO/Commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Giulio De Donato",
            "email": "liuggio@gmail.com"
        },
        {
            "name": "Simone Di Maulo",
            "email": "toretto460@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/validator": "^2.7|^3.0",
        "liuggio/filler-dto": "^0.1.0",
        "doctrine/common": "^2.6",
        "psr/http-message": "^1.0"
    },
    "suggest": {
        "symfony/property-access": "Required to use ConvertingObjectMapper"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7|~5.0",
        "symfony/http-foundation": "^3.0"
    },
    "autoload": {
        "psr-4": { "Bindto\\": "src"}
    },
    "autoload-dev": {
        "psr-4": { "Bindto\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}
